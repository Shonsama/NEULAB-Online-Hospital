<!--pages/zhuce/zhuce.wxml-->
<cu-custom bgColor="bg-gradual-blue" isBack="{{true}}">
	<view slot="backText">返回</view>
	<view slot="content">注册</view>
</cu-custom>
<form>
<view class="cu-form-group margin-top">
		<view class="title">用户名</view>
		<input bindinput="useridInput"></input>
	</view>
	<view class="cu-form-group">
		<view class="title">密码</view>
		<input bindinput="passwordInput"></input>
	</view>
	<view class="cu-form-group">
		<view class="title">姓名</view>
		<input bindinput="nameInput"></input>
	</view>
	<view class="cu-form-group">
		<view class="title">性别</view>
		<input bindinput="genderInput"></input>
	</view>
  <view class="cu-form-group">
		<view class="title">家庭地址</view>
		<input bindinput="addressInput"></input>
	</view>
	<view class="cu-form-group">
		<view class="title">出生日期</view>
		<input bindinput="birthInput"></input>
	</view>
	<view class="cu-form-group">
		<view class="title">年龄</view>
		<input bindinput="ageInput"></input>
	</view>
  <view class="cu-form-group">
		<view class="title">身份证号</view>
		<input bindinput="idInput"></input>
	</view>
  <view class="padding flex flex-direction">
  <button class="cu-btn bg-blue lg">注册</button>
</view>
</form>
